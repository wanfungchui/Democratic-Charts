<%- if controller_name != 'sessions' %>
  <%= link_to "I already have an account.", new_session_path(resource_name), class: "btn btn-default btn-gap" %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Don't have an account?", new_registration_path(resource_name), class: "btn btn-default btn-gap" %><br />
<% end -%>
